<div class="table-responsive">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th></th>
        <th>Name</th>
        <th>Total Tracks</th>
        <th>Rdio URL</th>
        <th>Radio Key</th>
        <th>Top Songs Key</th>
        <th>Key</th>
        <th>Artist Key</th>
        <!-- <th>Count</th> -->
        <!-- <th>Album Count</th> -->
      </tr>
    </thead>
    <tbody>
      <% @albums.each do |album| %>
        <tr>
          <td>
            <img src="<%= album["icon"] %>" height="40" width="40" />
          </td>
          <td>
            <a href="/artists/<%= album["key"] %>"><%= album["name"] %></a>
          </td>
          <td>
            <a href="#"><%= album["length"] %></a>
          </td>
          <td>
            <a href="<%= album["shortUrl"] %>" target="lbrary-artist-short-url">View in Rdio</a>
          </td>
          <td>
            <a href="#"><%= album["radioKey"] %></a>
          </td>
          <td>
            <a href="#"><%= album["topSongsKey"] %></a>
          </td>
          <td>
            <a href="#"><%= album["key"] %></a>
          </td>
          <td>
            <a href="#"><%= album["artistKey"] %></a>
          </td>
          <!-- <td>
            <a href="#"><%#= album["count"] %></a>
          </td>
          <td>
            <a href="#"><%#= album["albumCount"] %></a>
          </td> -->
        </tr>
      <% end %>
    </tbody>
  </table>
</div>