<div class="table-responsive">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th>Key</th>
        <!-- <th>Type</th> -->
        <th>URL</th>
        <th>Length</th>
        <th>Icon</th>
        <th>Base Icon</th>
        <th>Has Radio?</th>
        <th>Short URL</th>
        <th>Radio Key</th>
        <th>Top Songs Key</th>
        <th>User Key</th>
        <!-- <th>User Name</th> -->
        <th>Artist Key</th>
        <th>Artist URL</th>
        <th>Count</th>
        <th>Album Count</th>
      </tr>
    </thead>
    <tbody>
      <% @artists.each do |artist| %>
        <tr>
          <td>
            <a href="#"><%= artist["name"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["key"] %></a>
          </td>
          <!-- <td>
            <a href="#"><%= artist["type"] %></a>
          </td> -->
          <td>
            <a href="#"><%= artist["url"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["length"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["icon"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["baseIcon"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["hasRadio"] %></a>
          </td>
          <td>
            <a href="<%= artist["shortUrl"] %>" target="lbrary-artist-short-url"><%= artist["shortUrl"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["radioKey"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["topSongsKey"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["userKey"] %></a>
          </td>
          <!-- <td>
            <a href="#"><%= artist["userName"] %></a>
          </td> -->
          <td>
            <a href="#"><%= artist["artistKey"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["artistUrl"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["count"] %></a>
          </td>
          <td>
            <a href="#"><%= artist["albumCount"] %></a>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>